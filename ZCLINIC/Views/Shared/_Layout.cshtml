    <script type="text/javascript">

        $('#online').show();
        $('#offline').hide();
        window.setInterval(function () {

            var x = navigator.onLine;
            if (x == false) {
                ErrorAlert("Internet is not Connected");
                $('#online').hide();
                $('#offline').show();
            }
            else {
