@using ZCLINIC
<div class="page-header page-header-default">
    <div class="breadcrumb-line">
        <h5 class="text-semibold">Panel Wise Student List</h5>
    </div>
</div>

@{
    string a = string.Empty;
}



<div class="content">
    <div class="panel panel-flat">
        <div class="panel-body">






















            <div class="row">


                <div class="form-group form-group-xs col-md-12">




                    <div class="form-group form-group-xs col-md-4">
                        <label>Panel Name<b class="wysiwyg-color-red"></b></label>
                        <div>
                            @{
                                List<ZCLINIC.Areas.Master.Models.Panel> Panel = new ZCLINIC.Areas.Master.Models.Panel().Master_PanelGetAll(Context.Session.GetBValues(), Context.GetIP(), Context.GetCInfo());
                                Panel.Insert(0, new ZCLINIC.Areas.Master.Models.Panel { PId = 0, PName = "ALL" });

                            }
                            @Html.DropDownList("PanelId", Panel.Select(p => new SelectListItem() { Text = p.PName, Value = p.PId.ToString() }), new { @class = "form-control", data_style = "btn-default btn-xs", tabindex = '4', data_search = "true", data_width = "100%" })

                        </div>
                    </div>




                </div>









            </div>

            <div class="row">


                <div class="form-group form-group-xs col-md-6">

                    <div class="form-group form-group-xs col-md-4">
                        <a id="SPrint" class="btn btn-primary btn-xs"><i class="position-left"></i>Generate Report</a>



                    </div>






                </div>



            </div>

        </div>

    </div>
</div>
<script>


    $('#SPrint').click(function (e) {



       
        var PanelName = $('#PanelId option:selected').text();






        var value = `/Report/Reportrdlc/AReport?rvalues.Name=46&rvalues.Value[0]=${$('#PanelId').val()}&rvalues.Value[1]=''&rvalues.Value[2]=''&rvalues.Value[3]=''&rvalues.Value[4]=''&rvalues.Value[5]=''&rvalues.Value[6]=${PanelName}`
        $(this).attr('href', value)
        $(this).attr('target', "_blank")




    })



</script>