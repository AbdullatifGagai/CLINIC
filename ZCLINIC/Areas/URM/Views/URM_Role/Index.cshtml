

@model IEnumerable<ZCLINIC.Areas.URM.Models.URM_Role>



<div class="page-header page-header-default">
    <div class="breadcrumb-line">
        <h6 class="text-semibold">User Role</h6>
    </div>
</div>


<div class="content">
    <div class="panel panel-flat">
        <div class="panel-body">
            <div class="row">
                <div class="col-md-3 buttons-top">



                    <a class="btn btn-success btn-sm" data-modalhorizontal="true" data-modalsize="modal-lg" data-url="/URM/URM_Role/AURM_Role" data-heading="Users Role" data-script="/assets/Areas/URM/Roles.js"><i class="icon-add mr-5"></i> Add</a>


                </div>

                <div class="col-sm-6">
                    <div class="form-group">
                        @*<select class="bootstrap-select data-type" data-style="btn-primary" data-width="20%">
                                <option value="0">All</option>
                                <option value="1">Active</option>
                                <option value="2">Inactive </option>
                            </select>*@

                        @*<select url="/Master/MasterReports/City" class="bootstrap-select" id="downloadBtn" data-style="btn-primary" data-width="30%">
                                <option value="0">[SELECT Type]</option>
                                <option value="1">PDF</option>
                                <option value="2">EXCEL</option>
                            </select>*@
                    </div>

                </div>

                <div class="col-sm-3">
                    <div class="input-group ">
                        <input type="text" class="form-control" id="FilterInput">
                        <span class="input-group-addon bg-blue-800"><i class="icon-search4"></i></span>
                    </div>
                </div>
            </div>

            <table class="table table-xxs table-md datatable-basic table-bordered table-striped table-hover bg-lightgray dataTables_scrollBody">

                <thead>
                    <tr class="bg-blue-800">
                        <th>RoleId</th>
                        <th>Roles Name</th>
                       






                        @*  <th>Status</th>*@
                        <th>Actions</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (ZCLINIC.Areas.URM.Models.URM_Role s in Model)
                    {
                        <tr data-rowid="@s.Roles_ID">
                            <td>@s.Roles_ID</td>
                            <td>@s.Roles_Name</td>
                          




                            <td>
                                <ul class="icons-list">
                                    <li class="text-primary-600"><a data-modalhorizontal="true" data-modalsize="modal-lg" data-heading="Update Users Role" data-script="/assets/Areas/URM/Roles.js" data-url="/URM/URM_Role/AURM_Role?Id=@s.Roles_ID"><i class="icon-pencil7"></i></a></li>
                                    <li class="text-danger-600"><a onclick="DeleteAlert({ 'Id':@s.Roles_ID}, '/URM/USERAUTH/DeleteUSERAUTH', 'Users')"><i class="icon-trash"></i></a></li>




                                </ul>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
