@model IEnumerable<ZCLINIC.Areas.URM.Models.Company>

<div class="page-header page-header-default">
    <div class="breadcrumb-line">
        <h6 class="text-semibold">Company</h6>
    </div>
</div>
@*@{

    string check;
}*@
<div class="content">
    <div class="panel panel-flat">
        <div class="panel-body">
            <div class="row">
                <div class="col-sm-9 buttons-top">
                    <a class="btn btn-success btn-sm" data-modalhorizontal="true" data-modalsize="modal-lg" data-url="/URM/Company/ACompany" data-heading="Company" data-script="#"><i class="icon-add mr-5"></i> Add</a>
                </div>
                <div class="col-sm-3">
                    <div class="input-group ">
                        <input type="text" class="form-control" id="FilterInput">
                        <span class="input-group-addon bg-blue-800"><i class="icon-search4"></i></span>
                    </div>
                </div>
            </div>

            <table class="table table-xxs table-md datatable-basic table-bordered table-striped table-hover bg-lightgray dataTables_scrollBody">
                <thead>
                    <tr class="bg-blue-800">
                        <th>Company Id</th>
                        <th>Company Name</th>
                        <th>Expair Date</th>
                        <th>AppId</th>
                       
                        <th>Actions</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (ZCLINIC.Areas.URM.Models.Company s in Model)
                    {
                        <tr data-rowid="@s.TenantId">
                            <td>@s.TenantId</td>
                            <td>@s.CompanyName</td>
                            <td>@s.Exdate</td>
                            <td>@s.AppId</td>
                           
                            <td>
                                <ul class="icons-list">
                                    <li class="text-primary-600"><a data-modalhorizontal="true" data-modalsize="modal-lg" data-heading="Edit Company" data-script="#" data-url="/URM/Company/ACompany?Id=@s.TenantId"><i class="icon-pencil7"></i></a></li>
                                    <li class="text-danger-600"><a onclick="DeleteAlert({ 'Id':@s.TenantId}, '/URM/Company/DeleteCompany', 'Company Name')"><i class="icon-trash"></i></a></li>
                                </ul>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
