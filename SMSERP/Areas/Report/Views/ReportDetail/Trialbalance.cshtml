@using ZCLINIC
<div class="page-header page-header-default">
    <div class="breadcrumb-line">
        <h5 class="text-semibold">Trial  Balance Report</h5>
    </div>
</div>

@{
    string a = string.Empty;
}



<div class="content">
    <div class="panel panel-flat">
        <div class="panel-body">


















            <div class="row">


                <div class="form-group form-group-xs col-md-12">

                    <div class="form-group form-group-xs col-md-2">
                        <label>From  Date<b class="wysiwyg-color-red"></b></label>
                        <input type="date" class="form-control" id="FromDate" />

                    </div>




                    <div class="form-group form-group-xs col-md-2">
                        <label>To Date<b class="wysiwyg-color-red"></b></label>
                        <input type="date" class="form-control" id="ToDate" />

                    </div>







                </div>




            </div>



            <div class="row">


                <div class="form-group form-group-xs col-md-12">




                    <div class="form-group form-group-xs col-md-4">
                        <label>Master Account<b class="wysiwyg-color-red"></b></label>
                        <div>
                            @{
                                List<ZCLINIC.Areas.Master.Models.MasterAccount> MasterAccount = new ZCLINIC.Areas.Master.Models.MasterAccount().Master_MasterAccountGetAll(Context.Session.GetBValues(), Context.GetIP(), Context.GetCInfo());
                                MasterAccount.Insert(0, new ZCLINIC.Areas.Master.Models.MasterAccount { Master_Code = 0.ToString(), Master_Name = "Select Master Account" });

                            }
                            @Html.DropDownList("Master_Code", MasterAccount.Select(p => new SelectListItem() { Text = p.Master_Name, Value = p.Master_Code.ToString() }), new { @class = "form-control", data_style = "btn-default btn-xs", tabindex = '4', data_search = "true", data_width = "100%" })

                        </div>
                    </div>





                </div>









            </div>

            <div class="row">


                <div class="form-group form-group-xs col-md-6">

                    <div class="form-group form-group-xs col-md-3">
                        <a id="Print" class="btn btn-primary btn-xs"><i class="position-left"></i>Generator Report Trial Balance Report</a>


                    </div>
                </div>



            </div>

        </div>

    </div>
</div>
