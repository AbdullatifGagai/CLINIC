@using ZCLINIC
<div class="page-header page-header-default">
    <div class="breadcrumb-line">
        <h5 class="text-semibold">Stock Report</h5>
    </div>
</div>

@{
    string a = string.Empty;
}



<div class="content">
    <div class="panel panel-flat">
        <div class="panel-body">


















            <div class="row">


                <div class="form-group form-group-xs col-md-12">

                    <div class="form-group form-group-xs col-md-2">
                        <label>From  Date<b class="wysiwyg-color-red"></b></label>
                        <input type="date" class="form-control" id="FromDate" />

                    </div>




                    <div class="form-group form-group-xs col-md-2">
                        <label>To Date<b class="wysiwyg-color-red"></b></label>
                        <input type="date" class="form-control" id="ToDate" />

                    </div>







                </div>




            </div>



            <div class="row">


                <div class="form-group form-group-xs col-md-12">




                    <div class="form-group form-group-xs col-md-4">
                        <label>Store Name<b class="wysiwyg-color-red"></b></label>
                        <div>
                            @{
                                List<ZCLINIC.Areas.Master.Models.Store> Store = new ZCLINIC.Areas.Master.Models.Store().Master_StoreGetAll(Context.Session.GetBValues(), Context.GetIP(), Context.GetCInfo());
                                Store.Insert(0, new ZCLINIC.Areas.Master.Models.Store { storeId = 0, Storename = "ALL" });

                            }
                            @Html.DropDownList("storeId", Store.Select(p => new SelectListItem() { Text = p.Storename, Value = p.storeId.ToString() }), new { @class = "form-control", data_style = "btn-default btn-xs", tabindex = '4', data_search = "true", data_width = "100%" })

                        </div>
                    </div>

                    
                    <div class="form-group form-group-xs col-md-4">
                        <label>Product  Name<b class="wysiwyg-color-red"></b></label>
                        <div>
                            @{
                                List<ZCLINIC.Areas.Master.Models.Product> Product = new ZCLINIC.Areas.Master.Models.Product().Master_ProductGetAll(Context.Session.GetBValues(), Context.GetIP(), Context.GetCInfo());
                                Product.Insert(0, new ZCLINIC.Areas.Master.Models.Product { ProductId = 0, ProductName = "ALL" });

                            }
                            @Html.DropDownList("ProductId", Product.Select(p => new SelectListItem() { Text = p.ProductName, Value = p.ProductId.ToString() }), new { @class = "form-control", data_style = "btn-default btn-xs", tabindex = '4', data_search = "true", data_width = "100%" })

                        </div>
                    </div>





                </div>









            </div>

            <div class="row">


                <div class="form-group form-group-xs col-md-6">

                    <div class="form-group form-group-xs col-md-4">
                        <a id="StockQtyPrint" class="btn btn-primary btn-xs"><i class="position-left"></i>Stock Report Qty Wise</a>
                     


                    </div>
                   
                
                
                



                </div>



            </div>

        </div>

    </div>
</div>
